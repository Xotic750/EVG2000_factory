#this config file builds single band for 11n chips

include $(WLCFGDIR)/wlconfig_lx_wl_dslcpepcisdcfg
include $(WLCFGDIR)/wlconfig_lx_wl_dslcpe

# features
DBAND=0

#supported chips 4321(x) 4322(x) 43222(x)
export SUPPORTED_CHIPS := 4321 4322 43222

#4321/4322 configuration
WLFLAGS += -DD11CONF=0x00011800
WLFLAGS += -DACONF=0
WLFLAGS += -DGCONF=0
WLFLAGS += -DNCONF=0x0000005f
WLFLAGS += -DLPCONF=0
