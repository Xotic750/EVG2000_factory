#this config file builds single band 11g chip

include $(WLCFGDIR)/wlconfig_lx_wl_dslcpepcisdcfg
include $(WLCFGDIR)/wlconfig_lx_wl_dslcpe
include $(WLCFGDIR)/wlconfig_nomimo

# features
WL11H=0
WL11D=0
DBAND=0
BCMDMA64=0
RXCHAIN_PWRSAVE=0

# chip specific
BCM4312=0

#supported chips 4318B0
export SUPPORTED_CHIPS := 4318

EXTRA_CFLAGS += -DD11CONF=0x300
EXTRA_CFLAGS += -DACONF=0
EXTRA_CFLAGS += -DGCONF=0x80
EXTRA_CFLAGS += -DNCONF=0
EXTRA_CFLAGS += -DLPCONF=0

